{
    "manifest_version": 3,
    "name": "nihongoTab",
    "version": "0.5",
    "description": "Practice Japanese vocab every time you open a new tab!",
    "permissions": [
        "storage",
        "activeTab"
    ],
    "chrome_url_overrides" : {
        "newtab": "index.html"
    },
    "background": {
        "service_worker": "background.js"
     },
     "icons": {
        "16": "assets/icon-16.png",
        "32": "assets/icon-32.png",
        "48": "assets/icon-48.png",
        "128": "assets/icon-128.png"
    }
  }