{
    "manifest_version": 3,
    "name": "nihongoTab",
    "description": "Practice Japanese vocab every time you open a new tab!",
    "version": "0.5",
    "icons": {
        "16": "assets/icon-16.png",
        "32": "assets/icon-32.png",
        "48": "assets/icon-48.png",
        "128": "assets/icon-128.png"
    },
    "chrome_url_overrides" : {
        "newtab": "index.html"
    },
    "permissions": [
        "storage"
    ]
}